{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "typeRoots": [
      "nativewind/types",
      "./node_modules/@types",
      "expo-router/ts-declarations"
    ],
    "types": [
      "node",
      "react"
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
